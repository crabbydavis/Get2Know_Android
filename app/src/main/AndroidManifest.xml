<?xml version="1.0" encoding="utf-8"?>
<manifest package="crabbman.get2know"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/get2know_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:minSdkVersion="16">
        <activity android:name=".UI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".UI.PlayerActivity"></activity>
        <activity android:name=".UI.QuestionActivity"></activity>
        <activity android:name=".UI.AnswerActivity"></activity>
        <activity android:name=".UI.GuessActivity"></activity>
        <activity android:name=".UI.GameOverActivity"></activity>
        <activity android:name=".UI.FreePlayActivity"></activity>
        <activity android:name=".UI.MainActivityPopUp" android:theme="@style/AppTheme.PopupTheme">
        </activity>
    </application>

</manifest>