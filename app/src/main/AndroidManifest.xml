<?xml version="1.0" encoding="utf-8"?>
<manifest package="crabbman.get_2_know"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/get2know_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:minSdkVersion="16"
        android:screenOrientation="portrait">
        <activity android:name="crabbman.get_2_know.UI.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="crabbman.get_2_know.UI.PlayerActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="crabbman.get_2_know.UI.QuestionActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="crabbman.get_2_know.UI.AnswerActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="crabbman.get_2_know.UI.GuessActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="crabbman.get_2_know.UI.GameOverActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="crabbman.get_2_know.UI.FreePlayActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="crabbman.get_2_know.UI.MainActivityPopUp" android:theme="@style/AppTheme.PopupTheme" android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>